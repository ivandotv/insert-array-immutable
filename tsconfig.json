{
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./.buildcache/lib.tsbuildinfo",
    "declaration": true,
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "./lib/",
    "rootDir": "./src/",
    "resolveJsonModule": true,
    "experimentalDecorators": true
  },
  "include": ["src/**/*.ts"]
}
